import java.beans.Expression;
import java.util.HashMap;
import java.util.Map;

import org.apache.commons.jexl2.JexlContext;
import org.apache.commons.jexl2.JexlEngine;
import org.apache.commons.jexl2.MapContext;



public class Main {

	
	public static void main(String[] args){
		System.out.println("1111");
		Map<String,Object> map=new HashMap<String,Object>(); 
		map.put("testService",System.out); 
		map.put("person","hello world"); 
		String expression="testService.print(person)"; 
		DyMethodUtil.invokeMethod(expression,map);
	}
	
}
class DyMethodUtil {
	 
	 public static Object invokeMethod(String jexlExp,Map<String,Object> map){
	  JexlEngine jexl=new JexlEngine();
	  Expression e = (Expression) jexl.createExpression(jexlExp);
	  JexlContext jc = new MapContext();
	  for(String key:map.keySet()){
	   jc.set(key, map.get(key));
	  }
	  if(null==((org.apache.commons.jexl2.Expression) e).evaluate(jc)){
	   return "";
	  }
	  return ((org.apache.commons.jexl2.Expression) e).evaluate(jc);
	 }
	 
	}