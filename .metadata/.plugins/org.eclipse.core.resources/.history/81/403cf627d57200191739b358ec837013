	}
	
	public void createData(){
		for(int i=0;i<this.data.getColumns();i++){
			this.data.set(0, i, Math.random()*5);
			this.Y.set(0, i, k);
		}
		
	}
	
	public void initMyExpression(String temp){
		this.myExpression = temp;
